{
    "name": "afiqiqmal/approval-laravel",
    "description": "approval-laravel",
    "keywords": [
        "afiqiqmal",
        "approval-laravel"
    ],
    "homepage": "https://github.com/afiqiqmal/approval-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Hafiq",
            "email": "hafiqiqmal93@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^6 || ^7 || ^8",
        "illuminate/support": "^6 || ^7 || ^8"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Afiqiqmal\\Approval\\": "src",
            "Afiqiqmal\\Approval\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Afiqiqmal\\Approval\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Afiqiqmal\\Approval\\ApprovalServiceProvider"
            ],
            "aliases": {
                "Approval": "Afiqiqmal\\Approval\\ApprovalFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
